<h1>Eat Da-Burger</h1>


<ul>
  {{#each burgers}}
    {{#unless devoured}}
      {{> cats/cat-block devoured=true}}
    {{/unless}}
  {{/each}}
</ul>

<form class="create-form">

  <div class="form-group">
    <label for="ca">Submit the burger you want to eat!</label>
    <input type="text" id="ca" name="name">
  </div>

  <button type="submit">Submit</button>
</form>
